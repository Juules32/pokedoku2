{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": false,
      "gh-pages": false
    }
  },
  "crons": [
    {
      "path": "/cron/dailyData/generate",
      "schedule": "0 22 * * *"
    }
  ]
}
